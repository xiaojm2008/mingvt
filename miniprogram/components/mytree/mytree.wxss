.ul {
  width: 100%;
}
.ul .li-item {
  width: inherit;
  height: 30px;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content:space-between;
  padding: 0 10px 0 5px;
  font-size:13px;
  font-weight: 500;
  color: white;
  border-bottom: 0.5rpx solid rgba(213, 213, 214, 0.5);
}
.ul .li-item_h{
  /*filter: grayscale(50%);*/
  background-color: rgba(248, 248, 250, 0.4);
}
.ul .li-item .left {
  text-align: left;
}
