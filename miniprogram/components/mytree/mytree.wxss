.icon-btn {
  width: 25px;
  height: 12px;
}

.iconfont {
  display: inline-block;
  font-family: 'iconfont' !important;
}

.icon-up:before {
  content: "\e956";
}

.icon-down:before {
  content: "\e958";
}

.icon-settings:before {
  content: "\e64b";
}
