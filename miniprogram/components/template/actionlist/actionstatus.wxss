.dict-flag{
  margin-right:5px; 
  /*display: inline-block;*/
  font-size:14px;
  height: 22px;
  line-height: 22px;
  border-radius: 5px;
  background-color: #fd0d0d;
  background-image:linear-gradient(45deg, #d41a1a, #d9e60c, #5c7c99);
  padding: 0 4px;
}
.st-flag{
  font-size:14px;
  color:green;
  border-radius: 5px;
  border: 1px solid green;
  line-height: 20px;
  height: 20px;
  /*display: inline-block;*/
  margin-right: 5px;
  padding: 0 4px
}
.st-gray{
  background-color: gray;
  border: 1px solid gray;
  color:white;
}
.st-red{
  background-color: rgb(209, 98, 98);
  border: 1px solid rgb(209, 98, 98);
  color:white;
}