.dialog-wrap{
  z-index: 51;
}
.dialog2{
  min-height: 550px;
  width: 100% !important;
}
.btn2{
  /*display: flex;
  align-items: center;
  justify-content:center;
  */
  display: inline-block;
  border: 1px solid rgb(10, 126, 10);
  border-radius: 8px;
  box-sizing: border-box;
  margin-right: 10px;
  width: 80px;
  height: 30px !important;
  line-height: 30px !important;
  text-align: center;
}

.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  display: inline-block;
}
.icon-plus:before {
  content: "\e931";
}
.icon-rightarrow:before {
  content: "\e94e";
}
.linetext {
  width: 100%;
  height: 30px;
  line-height: 30px;
  color: #999;
  font-size: 12px;
}