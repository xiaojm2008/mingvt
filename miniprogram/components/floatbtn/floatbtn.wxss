.icon-btn2 {
  position: fixed;
  width: 42px;
  height: 42px;
  bottom: 30px;
  right: 20px;
  background: #ff6700;
  line-height: 42px;
  font-size: 30px;
  color: #fff;
  border-radius: 42px;
}

.icon-btn {
  display: block;
  position: fixed; 
  right: 16px;
  bottom: 88px;
  width: 42px;
  height: 42px;
  font-size: 30px;
  font-weight: 500;
  text-align: center;
  line-height: 42px;
  border-radius: 50%;
  z-index: 150;
  overflow: hidden;
  transition-duration: 300ms;
  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23);
  background-color: #2196f3;
  color: #fff;
}
.iconfont {
  display: inline-block;
  font-family: 'iconfont' !important;
}

.icon-plus:before {
  content: "\e931";
}
